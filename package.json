{
  "name": "@niicojs/excel",
  "version": "0.3.0",
  "description": "typescript library to manipulate excel files",
  "homepage": "https://github.com/niicojs/excel#readme",
  "bugs": {
    "url": "https://github.com/niicojs/excel/issues"
  },
  "license": "MIT",
  "author": "niico",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/niicojs/excel.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "tsdx dev",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "lint": "tsdx lint",
    "format": "tsdx format",
    "format:check": "tsdx format --check",
    "typecheck": "tsdx typecheck",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3",
    "fflate": "^0.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "bunchee": "^6.9.4",
    "tsdx": "latest",
    "typescript": "^5.7.2",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20"
  }
}
